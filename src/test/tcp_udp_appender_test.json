{
	"layoutPattern": "${yyyy}-${MM}-${dd} ${HH}:${mm}:${ss}:${ms} [${8TN}] [${L}] -- ${W}",
	"appenders": {
		"consoleAppender": {
			"outStream": "stdout"
		},
		"tcpAppender": {
			"localAddr": "0.0.0.0",
			"port": 9443
		},
		"udpAppender": {
			"localAddr": "0.0.0.0",
			"port": 9443
		}
	},
	"layouts": [
		{
			"name": "tcpLayout",
			"logLevel": "info",
			"appenders": [
				"consoleAppender",
				"tcpAppender"
			]
		},
		{
			"name": "udpLayout",
			"logLevel": "info",
			"appenders": [
				"consoleAppender",
				"udpAppender"
			]
		}
	],
	"rootLayout": {
		"logLevel": "warn",
		"appenders": [
			"consoleAppender",
			"tcpAppender",
			"udpAppender"
		]
	}
}