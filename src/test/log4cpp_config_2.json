{
	"layoutPattern": "${yyyy}-${MM}-${dd} ${h}:${mm}:${ss}:${ms} [${8TN}] [${L}] -- ${W}",
	"appenders": {
		"consoleAppender": {
			"outStream": "stdout"
		},
		"fileAppender": {
			"filePath": "log/log4cpp_config_2.log"
		},
		"tcpAppender": {
			"localAddr": "2001:0db8:85a3:0000:0000:8a2e:0370:7334",
			"port": 9443
		},
		"udpAppender": {
			"localAddr": "::1",
			"port": 9443
		}
	},
	"rootLayout": {
		"logLevel": "info",
		"appenders": [
			"consoleAppender",
			"fileAppender",
			"tcpAppender",
			"udpAppender"
		]
	}
}