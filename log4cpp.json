{
  "log4cpp": {
	"outputters": {
	  "consoleOutputter": {
		"logLevel": "debug"
	  },
	  "fileOutputter": {
		"filePath": "log/log4cpp.log",
		"async": true,
		"append": false
	  }
	},
	"loggers": [
	  {
		"name": "main",
		"logLevel": "info",
		"outputter": [
		  "consoleOutputter"
		]
	  },
	  {
		"name": "test",
		"logLevel": "error",
		"outputter": [
		  "consoleOutputter",
		  "fileOutputter"
		]
	  }
	],
	"root": {
	  "pattern": 1,
	  "logLevel": "info",
	  "outputter": [
		"consoleOutputter",
		"fileOutputter"
	  ]
	}
  }
}